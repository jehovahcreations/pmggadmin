<!DOCTYPE html>
<html lang="en">

<head>
    <title>User Table</title>
</head>

<body>
    <div class="table-data">
        <h2>User Table</h2>
        <table border="1">
            <tr>
                <th>S.N</th>
                <th>Name</th>
                <th>Email Address</th>

            </tr>

            <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
                <tr>
                    <td>
                        <%=i; %>
                    </td>
                    <td>
                        <%=data.username %>
                    </td>
                    <td>
                        <%=data.email %>
                    </td>


                    <td><a href="/users/delete/<%=data.id%>">Delete</a></td>
                </tr>
                <%  i++; }) %>
                    <% } else{ %>
                        <tr>
                            <td colspan="7">No Data Found</td>
                        </tr>
                        <% } %>
        </table>
    </div>
</body>

</html>